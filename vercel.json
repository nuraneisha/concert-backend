{
    "version": 2,
    "builds": [
        {
            "src": "server/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "server/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/server/(.*)",
            "dest": "/server/$1.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}